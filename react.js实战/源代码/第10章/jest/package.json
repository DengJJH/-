{
  "name": "jest",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "testRegex": ".*\\.spec\\.js$",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "src/**/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "test/spec/__mocks__/fileMock.js",
      "\\.(css|scss)$": "test/spec/__mocks__/styleMock.js"
    },
    "setupFiles": [
      "test/spec/setup.js"
    ]
  },
  "author": "",
  "license": "ISC",
  "keywords": [],
  "description": "",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^23.6.0",
    "regenerator-runtime": "^0.13.1"
  }
}
